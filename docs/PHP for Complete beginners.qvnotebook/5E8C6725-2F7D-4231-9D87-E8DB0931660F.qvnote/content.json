    },
    {
      "type": "code",
      "language": "php",
      "data": "<?php\n  $number = array('juanita', 'maria', 'jose');\n  \n  echo $number[2] . \"<br>\";\n  print_r($number);\n  echo \"<br>\";\n  \n  $names = array(\"first_name\" => 'Mer');\n  print_r($names);\n  echo \"<br>\";\n  echo $names[first_name];\n?>"
